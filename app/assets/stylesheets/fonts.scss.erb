@font-face {
    font-family: 'AndikaBasicRegular';
    src: url('<%= asset_path('AndBasR-webfont.eot') %>');
    src: url('<%= asset_path('AndBasR-webfont.eot?#iefix') %>') format('embedded-opentype'),
         url('<%= asset_path('AndBasR-webfont.woff') %>') format('woff'),
         url('<%= asset_path('AndBasR-webfont.ttf') %>') format('truetype'),
         url('<%= asset_path('AndBasR-webfont.svg#AndikaBasicRegular') %>') format('svg');
    font-weight: normal;
    font-style: normal;

}


